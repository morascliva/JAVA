

class Cuboid {
    private int length;
    private int width;
    private int height;

    public void set_length(int l) {
        this.length = l;
    }

    public void set_width(int w) {
        this.width = w;
    }

    public void set_height(int h) {
        this.height = h;
    }

    public void volume() {
        int volume = length * width * height;
        System.out.println(volume);
    }
}
