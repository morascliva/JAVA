class Solution {
    public int DiagonalSum(int[][] matrix) {
        int n = matrix.length;
        int primaryDiagonalSum = 0;
        int secondaryDiagonalSum = 0;

       
        for (int i = 0; i < n; i++) {
            primaryDiagonalSum += matrix[i][i];
        }

       
        for (int i = 0; i < n; i++) {
            secondaryDiagonalSum += matrix[i][n - 1 - i];

        }

        
        return primaryDiagonalSum + secondaryDiagonalSum;
    }
}
