
class GFG {
    String longest(String names[], int n) {
        if(n<=0 || n==null)
        {
            return null;
        }
        
        
        String longestname=names[0];
        
        
        
        for(int i=0;i<n;i++)
        {
            if(name[i]>longestname)
            {
                longestname =name[i];
            }
        }
        
        
        return longestname;
    }
}
