//User function Template for Java

class Solution {

    public void scores(long a[], long b[]) {
        long scoreA = 0;
        long scoreB = 0;

        for (int i = 0; i < a.length; i++) {
            if (a[i] > b[i]) {
                scoreA++;
            } else if (a[i] < b[i]) {
                scoreB++;
            }
            
        }

        GFG.ca=scoreA;
        GFG.cb=scoreB;
    }
}
