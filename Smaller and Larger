class Solve {
    int[] getMoreAndLess(int[] arr, int n, int x) {
        // code here
        int [] ans=new int[2];
        int countofSmaller=0;
        int countofLarger=0;
        
        for(int i=0;i<n;i++){
            if(arr[i]<=x){
                countofSmaller++;
            }
            if(arr[i]>=x){
                countofLarger++;
            }
        }
        ans[0]=countofSmaller;
        ans[1]=countofLarger;
        return ans;
    }
}
