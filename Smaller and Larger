

class Solve {
    int[] getMoreAndLess(int[] arr, int n, int x) {
        int count1 = 0;
        int count2 = 0;

        for (int i = 0; i < n; i++) {
            if (x < arr[i]) {
                count1++;
            } else if (x > arr[i]) {
                count2++;
            }
        }

        // Store the counts in an array and return it
        int[] result = {count1, count2};
        return result;
    }
}
