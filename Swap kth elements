
class Solution {
    void swapKth(int arr[], int n, int k) {
        if (k <= 0 || k > n) {
            // Invalid value of k
            return;
        }
        
        // Swap the kth element from the beginning with kth element from the end
        int temp = arr[k - 1];
        arr[k - 1] = arr[n - k];
        arr[n - k] = temp;
    }
}
