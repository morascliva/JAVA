
class Sol {
    Boolean check(String s) {
        
        if (s.length() <= 1) {
            return true;
        }
        
        
        char firstChar = s.charAt(0);
        
      
        for (int i = 1; i < s.length(); i++) {
           
            if (s.charAt(i) != firstChar) {
                return false;
            }
        }
        
        
        return true;
    }
}

